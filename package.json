{
    "title": "Zikai Liu Portfolio",
    "name": "zikai-liu-portfolio",
    "version": "1.1.0",
    "scripts": {
        "build": "npm run clean && npm-run-all --parallel build:css build:js",
        "build:js": "npm run rollup-js && npm run minify-js",
        "rollup-js": "rollup src/js/scripts.js -o js/scripts.js -f umd --name bootstrap -p @rollup/plugin-node-resolve --sourcemap",
        "minify-js": "terser --compress passes=2 --mangle --comments '/^!/' --source-map 'content=js/scripts.js.map,includeSources,url=scripts.min.js.map' --output js/scripts.min.js js/scripts.js",
        "build:css": "npm run render-css && npm run punge-css",
        "render-css": "node scripts/build-scss.js",
        "punge-css": "node scripts/punge-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "A grayscale one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://startbootstrap.com/theme/grayscale",
    "bugs": {
        "url": "https://github.com/StartBootstrap/startbootstrap-grayscale/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Zikai Liu",
    "contributors": [
        "David Miller (https://davidmiller.io/)",
        "Start Bootstrap"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/StartBootstrap/startbootstrap-grayscale.git"
    },
    "dependencies": {
        "bootstrap": "5.1.3"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.3.0",
        "autoprefixer": "10.4.4",
        "browser-sync": "^2.27.11",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "gsap": "^3.11.4",
        "npm-run-all": "^4.1.5",
        "postcss": "8.4.12",
        "prettier": "2.6.0",
        "pug": "3.0.2",
        "purgecss": "^4.1.3",
        "rollup": "^2.77.2",
        "sass": "1.49.9",
        "shelljs": "0.8.5",
        "terser": "^5.14.2",
        "upath": "2.0.1"
    },
    "browserslist": [
        "cover 99.5%"
    ]
}
